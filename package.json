{
  "name": "jtcarrasco",
  "version": "1.0.0",
  "description": "The website of jtcarrasco",
  "keywords": [
    "11ty",
    "SASS",
    "SCSS",
    "eleventy"
  ],
  "author": "Jason Carrasco",
  "license": "ISC",
  "scripts": {
    "watch:sass": "node-sass --watch src/sass -o src/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "node-sass src/sass -o src/css",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass --parallel build:*"
  },
  "dependencies": {
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5"
  },
  "main": ".eleventy.js",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtcarrasco/11ty-practice.git"
  },
  "bugs": {
    "url": "https://github.com/jtcarrasco/11ty-practice/issues"
  },
  "homepage": "https://github.com/jtcarrasco/11ty-practice#readme"
}
